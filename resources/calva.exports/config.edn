{:customREPLCommandSnippets
 [{:name "Show logs for instrumented function",
   :key "postmortem-show-logs-for-instrumented-fn"
   :snippet "(do (require 'postmortem.core) (postmortem.core/log-for `$top-level-defined-symbol))"}
  {:name "Print logs for instrumented function",
   :key "postmortem-print-logs-for-instrumented-fns"
   :snippet "(do (require 'clojure.pprint 'postmortem.core) (clojure.pprint/print-table [:args :ret]) (postmortem.core/log-for `$top-level-defined-symbol))"}
  {:name "Show logs for current instrumented function",
   :key "postmortem-show-logs-for-current-instrumented-fn"
   :snippet "(do (require 'postmortem.core) (postmortem.core/log-for `$current-fn))"}
  {:name "Print logs for current instrumented function",
   :key "postmortem-print-logs-for-current-instrumented-fn"
   :snippet "(do (require 'clojure.pprint 'postmortem.core) (clojure.pprint/print-table [:depth :args :ret]) (postmortem.core/log-for `$current-fn))"}
  {:name "Instrument current function",
   :key "postmortem-instrument-current-fn"
