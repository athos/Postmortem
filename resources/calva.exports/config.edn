  {:name "Show logs for current key",
   :key "postmortem-show-logs-for-current-key"
   :snippet "(do (require 'postmortem.core) (postmortem.core/log-for `$current-form))"},
  {:name "Print logs for current key",
   :key "postmortem-print-logs-for-current-key"
   :snippet "(do (require 'clojure.pprint 'postmortem.core) (postmortem.core/log-for `$current-form))"},
  {:name "Show all logs",
   :key "postmortem-show-all-logs"
   :snippet "(do (require 'postmortem.core) (postmortem.core/logs))"}
  {:name "Reset logs for current key",
